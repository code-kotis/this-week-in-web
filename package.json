{
  "name": "this-week-in-web",
  "private": true,
  "description": "Get weekly coverage about what's happening in web community. No spams.",
  "version": "0.1.0",
  "author": "Code Kotis",
  "dependencies": {
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-google-analytics": "^2.0.15",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-no-sourcemaps": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sass": "^2.0.8",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.2.3",
    "gatsby-transformer-sharp": "^2.1.8",
    "generate-newsletter": "1.0.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "this-week-in-web",
    "newsletter"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "generate": "generate-newsletter --out=./src/pages/issues",
    "format": "prettier --write \"src/**/*.js\"",
    "pre-commit": "lint-staged",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "lint-staged": {
    "\"src/**/*.js\"": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "lint-staged": "^8.1.7",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-kotis/this-week-in-web"
  },
  "bugs": {
    "url": "https://github.com/code-kotis/this-week-in-web/issues"
  },
  "workspaces": [
    "packages/*"
  ]
}
